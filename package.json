{
	"name": "ngrok-connect",
	"displayName": "ngrok-connect",
	"description": "An unofficial Ngrok extension that let's you see all the tunnels and connect to SSH ones",
	"version": "0.0.3",
	"publisher": "wassimbenzarti",
	"repository": {
		"type": "git",
		"url": "https://github.com/WassimBenzarti/Ngrok-connect.git"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"author": {
		"email": "m.wassim.benzarti@gmail.com",
		"name": "Wassim Benzarti",
		"url": "https://github.com/WassimBenzarti"
	},
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"tunnel-explorer": [
				{
					"id": "tunnelExplorerView",
					"name": "Tunnels"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "tunnel-explorer",
					"title": "Tunnel Explorer",
					"icon": "media/tunnel.svg"
				}
			]
		},
		"commands": [
			{
				"command": "extension.setSshConfig",
				"title": "Update SSH config",
				"category": "Ngrok Connect"
			},
			{
				"command": "extension.refresh",
				"title": "Refresh",
				"icon": "$(refresh)",
				"category": "Ngrok Connect"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "extension.refresh",
					"when": "view == tunnelExplorerView",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"deploy": "vsce publish --yarn"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/node-fetch": "^2.5.5",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vsce": "^1.74.0",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"node-fetch": "^2.6.7",
		"ssh-config": "^4.0.2"
	}
}